    <activity>
        <work>
            <nonIterativeScripting>
                <scriptExecutionRequest xmlns:s="http://midpoint.evolveum.com/xml/ns/public/model/scripting-3">
                    <s:execute>
                        <s:forWholeInput>true</s:forWholeInput>
                        <s:script>
                            <code>
                                import com.evolveum.midpoint.prism.PrismConstants
                                import com.evolveum.midpoint.xml.ns._public.common.common_3.*

                                TRIGGER_FIRE_AFTER = 60000
                                TRIGGER_SAFETY_MARGIN = 10000
                                
                               log.info("Recomputing tree with root at {}", orgOid)
                                root = midpoint.getObject(OrgType.class, orgOid)
                                children = [ root ]
                                while(!children.isEmpty()) {
                                    children_new = []
                                    for(child in children) {
                                        log.info('Recomputing org: {}', child.name)
                                        midpoint.recompute(OrgType.class, child.oid)
                                        query = prismContext.queryFor(OrgType.class).isDirectChildOf(child.oid).build()
                                        children_new.addAll(midpoint.searchObjects(OrgType.class, query))

                                        log.info('Looking for members of {}', child.oid)
                                        query = midpoint.prismContext.queryFor(UserType.class)
                                                .item(FocusType.F_ROLE_MEMBERSHIP_REF)
                                                .ref(child.oid, OrgType.COMPLEX_TYPE, PrismConstants.Q_ANY)
                                                .build()
                                        users = midpoint.searchObjects(UserType.class, query)
                                        log.info('Users found: {}', users.size())

                                        midpoint.searchObjectsIterative(UserType.class, query, { object, parentResult -&gt;
                                            log.info('Recomputing user {}', object.name)
                                            midpoint
                                                    .getOptimizingTriggerCreator(TRIGGER_FIRE_AFTER, TRIGGER_SAFETY_MARGIN)
                                                    .createForObject(UserType.class, object.oid)
                                            return true
                                        })
                                    }
                                    children = children_new
                                }

                        </code>
                        </s:script>
                    </s:execute>
                    <s:variables>
                        <s:variable>
                            <s:name>orgOid</s:name>
                            <s:expression>
                                <path>$task/extension/orgOid</path>
                            </s:expression>
                        </s:variable>
                    </s:variables>
                </scriptExecutionRequest>
            </nonIterativeScripting>
        </work>
